  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.9.4",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "ejs-cli": "^2.2.3"
  }
}
